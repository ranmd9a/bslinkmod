{
	"name": "__MSG_extName__",
	"version": "0.0.5",
	"manifest_version": 2,
	"description": "__MSG_description__",
	"default_locale": "en",
	"icons": {
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"permissions": [
		"background",
		"downloads"
	],
	"background": {
		"scripts": ["js/bg.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": [
				"https://beatsaver.com/*",
				"https://cdn.beatsaver.com/*"
			],
			"js": [
				"js/content.js"
			]
		}
	]
}
